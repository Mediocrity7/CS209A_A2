<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.controller.Controller">
   <Button fx:id="button_connect" layoutX="10.0" layoutY="8.0" lineSpacing="5.0" mnemonicParsing="false"
           onAction="#connectToServer" prefHeight="36.0" prefWidth="138.0" text="连接服务器"/>
